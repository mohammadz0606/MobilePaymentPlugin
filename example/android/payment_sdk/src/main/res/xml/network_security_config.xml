<?xml version="1.0" encoding="utf-8"?>
<network-security-config>
    <domain-config cleartextTrafficPermitted="true">
        <domain includeSubdomains="true">3.7.21.24</domain>
    </domain-config>
    <base-config>
        <trust-anchors><!-- Trust preinstalled CAs -->
            <certificates src="system" /><!-- Additionally trust user added CAs -->
            <certificates src="user" />
        </trust-anchors>
    </base-config>
</network-security-config>
